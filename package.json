{
  "version": "1.0.10",
  "name": "radix-ui-themes-with-tailwind",
  "description": "Tailwind plugin for Radix Themes",
  "main": "./dist/index.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
